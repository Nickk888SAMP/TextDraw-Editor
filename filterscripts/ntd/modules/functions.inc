			KillTimer(User[User_ChangingVarsTimer]);
		User[User_ChangingVarsTimer] = SetTimer("ChangingVarsTime", CHANGING_VAR_TIME, true);
	}
	else
	{
		if(User[User_ChangingVarsTimer] != -1)
			KillTimer(User[User_ChangingVarsTimer]);
		User[User_ChangingVarsTimer] = -1;
	}
	return 1;
}

